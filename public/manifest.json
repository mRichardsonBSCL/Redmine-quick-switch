{
  "manifest_version": 3,
  "name": "BitonRedmineViewer",
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "version": "0.1.0",
  "description": "Extension to quickly open Biton Redmine pages",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "192": "icons/icon_192.png",
    "128": "icons/icon_128.png",
    "512": "icons/icon_512.png"
  },
  "action": {
    "default_title": "RedmineViewer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key" : {
       "default":  "Alt+Shift+R"
      },
      "description": "Activate the extension"
    }
  }
}